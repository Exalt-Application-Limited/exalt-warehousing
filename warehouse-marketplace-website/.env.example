# Application Configuration
REACT_APP_NAME=Exalt Warehouse Marketplace
REACT_APP_VERSION=$npm_package_version
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG=true

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8080/api/v1
REACT_APP_WAREHOUSE_MARKETPLACE_API_URL=http://localhost:8083/api/v1
REACT_APP_STORAGE_MANAGEMENT_API_URL=http://localhost:8084/api/v1
REACT_APP_BILLING_SERVICE_URL=http://localhost:8200/api/v1
REACT_APP_INVENTORY_SERVICE_URL=http://localhost:8081/api/v1
REACT_APP_FULFILLMENT_SERVICE_URL=http://localhost:8082/api/v1
REACT_APP_ANALYTICS_SERVICE_URL=http://localhost:8085/api/v1
REACT_APP_AUTH_SERVICE_URL=http://localhost:8086/api/v1
REACT_APP_NOTIFICATION_SERVICE_URL=http://localhost:8087/api/v1

# WebSocket Configuration
REACT_APP_WEBSOCKET_URL=ws://localhost:8080/ws
REACT_APP_ENABLE_WEBSOCKET=true

# Authentication
REACT_APP_AUTH_DOMAIN=auth.warehousemarketplace.com
REACT_APP_AUTH_CLIENT_ID=warehouse-marketplace-web-client
REACT_APP_AUTH_REDIRECT_URI=http://localhost:3001/auth/callback
REACT_APP_AUTH_SCOPE=openid profile email
REACT_APP_TOKEN_STORAGE_KEY=exalt_warehouse_token
REACT_APP_REFRESH_TOKEN_KEY=exalt_warehouse_refresh_token

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_SOCIAL_LOGIN=true
REACT_APP_ENABLE_GEOLOCATION=true
REACT_APP_ENABLE_MAP_SEARCH=true
REACT_APP_ENABLE_VIRTUAL_TOURS=true
REACT_APP_ENABLE_REVIEWS=true
REACT_APP_ENABLE_LIVE_CHAT=true
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=true
REACT_APP_ENABLE_FACILITY_COMPARISON=true
REACT_APP_ENABLE_UNIT_CALCULATOR=true

# UI Configuration
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORTED_LANGUAGES=en,es,fr,de,zh,ar
REACT_APP_DEFAULT_CURRENCY=USD
REACT_APP_SUPPORTED_CURRENCIES=USD,EUR,GBP,CAD,AUD
REACT_APP_DATE_FORMAT=MM/DD/YYYY
REACT_APP_TIME_FORMAT=HH:mm:ss

# Search Configuration
REACT_APP_DEFAULT_SEARCH_RADIUS=25
REACT_APP_MAX_SEARCH_RADIUS=100
REACT_APP_DEFAULT_PAGE_SIZE=20
REACT_APP_MAX_PAGE_SIZE=100

# Geolocation & Maps
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
REACT_APP_MAPBOX_API_KEY=your-mapbox-api-key
REACT_APP_DEFAULT_LATITUDE=40.7128
REACT_APP_DEFAULT_LONGITUDE=-74.0060
REACT_APP_DEFAULT_ZOOM=12
REACT_APP_ENABLE_GPS_LOCATION=true

# Storage Unit Configuration
REACT_APP_MIN_UNIT_SIZE=25
REACT_APP_MAX_UNIT_SIZE=1000
REACT_APP_SIZE_UNITS=sq_ft,sq_m
REACT_APP_MIN_RENTAL_PERIOD=30
REACT_APP_MAX_RENTAL_PERIOD=365

# File Upload
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp,application/pdf

# Payment Configuration
REACT_APP_STRIPE_PUBLIC_KEY=pk_test_xxxxxxxxxxxxx
REACT_APP_PAYPAL_CLIENT_ID=your-paypal-client-id
REACT_APP_ENABLE_ACH_PAYMENTS=true
REACT_APP_ENABLE_CRYPTO_PAYMENTS=false

# Facility Features
REACT_APP_FACILITY_FEATURES=climate_control,24_hour_access,security_cameras,drive_up_access,elevator_access,first_floor,ground_floor,covered_loading,boxes_supplies,moving_truck,dollies_carts,residential_friendly,business_friendly

# Analytics
REACT_APP_GA_TRACKING_ID=UA-XXXXXXXXX-X
REACT_APP_GTM_ID=GTM-XXXXXXX
REACT_APP_MIXPANEL_TOKEN=your-mixpanel-token
REACT_APP_HOTJAR_ID=your-hotjar-id

# Social Media
REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id

# CDN Configuration
REACT_APP_CDN_URL=https://cdn.warehousemarketplace.com
REACT_APP_IMAGE_CDN_URL=https://images.warehousemarketplace.com
REACT_APP_FACILITY_IMAGES_CDN=https://facility-images.warehousemarketplace.com

# Error Tracking
REACT_APP_SENTRY_DSN=https://xxxxxxxxx@sentry.io/xxxxxxx

# Performance Monitoring
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_PERFORMANCE_SAMPLE_RATE=0.1

# Security
REACT_APP_ENABLE_CSP=true
REACT_APP_CSP_REPORT_URI=https://csp.warehousemarketplace.com/report

# Cache Configuration
REACT_APP_CACHE_TTL=3600
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_FACILITY_CACHE_TTL=1800
REACT_APP_SEARCH_CACHE_TTL=900

# Development Tools
REACT_APP_ENABLE_REDUX_DEVTOOLS=true
REACT_APP_ENABLE_REACT_DEVTOOLS=true

# Business Configuration
REACT_APP_COMPANY_NAME=Exalt Warehousing
REACT_APP_COMPANY_PHONE=1-800-STORAGE
REACT_APP_COMPANY_EMAIL=support@warehousemarketplace.com
REACT_APP_SUPPORT_HOURS=24/7
REACT_APP_EMERGENCY_CONTACT=1-800-EMERGENCY